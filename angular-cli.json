        "assets/vendor/moment/moment.min.js",
        "assets/vendor/sliderTabs/jquery.sliderTabs.js",
        "assets/vendor/fullcalendar/fullcalendar.js",
        "assets/vendor/fullcalendar/locale/th.js",
        "assets/vendor/retro/js/jquery.smooth-scroll.min.js",
        "assets/vendor/retro/js/bootstrap.min.js",
        "assets/vendor/retro/js/retro.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
